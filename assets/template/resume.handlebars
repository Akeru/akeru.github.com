<div class="row">
    <div class="span3">
        <div class="sidebar-content">
            <div class="row-fluid">
                <div class="span10">
                    <h2>{{bio.name}}</h2>
                </div>
                <div class="span2 hidden-print">
                    <div class="btn-toolbar">
                        <div class="btn-group">
                            <button class="btn dropdown-toggle" data-toggle="dropdown"><i class="fa fa-cog"></i>
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu">
                                <li><a id="language-english" href="#">English</a></li>
                                <li><a id="language-french" href="#">Fran√ßais</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row-fluid">
                <div class="span12">
                    <strong>{{section.headline}}</strong>
                </div>
            </div>

            {{#profiles}}
                <div class="row-fluid">
                    <div class="span12">
                        {{description}}
                    </div>
                </div>
            {{/profiles}}

            <div class="row-fluid">
                <div class="span12">
                    <strong>{{section.contact}}</strong>
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-mobile-phone fa-fw"></i>
                </div>
                <div class="span11">
                    <span class="obfuscated encrypted" data-trim="22">{{contact.phone}}</span>
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-envelope fa-fw"></i>
                </div>
                <div class="span11">
                    <span class="obfuscated encrypted" data-trim="22">{{contact.email}}</span>
                </div>
            </div>

            {{#contact.locations}}
                <div class="row-fluid">
                    <div class="span1">
                        {{#if @first}}
                        <i class="fa fa-map-marker fa-fw"></i>
                        {{/if}}
                    </div>
                    <div class="span11">
                        <span class="obfuscated encrypted" data-trim="22">{{description}}</span>
                    </div>
                </div>
            {{/contact.locations}}

            <div class="row-fluid">
                <div class="span12">
                    <strong>{{section.misc}}</strong>
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-flag-o fa-fw"></i>
                </div>
                <div class="span11">
                    {{bio.nationalty}}
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-gift fa-fw"></i>
                </div>
                <div class="span11">
                    <span class="obfuscated encrypted" data-trim="15">{{bio.birthdate}}</span>
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-female fa-fw"></i>
                </div>
                <div class="span11">
                    {{bio.marital}}
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-car fa-fw"></i>
                </div>
                <div class="span11">
                    {{bio.car}}
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-linkedin fa-fw"></i>
                </div>
                <div class="span11">
                    <a href='http://goo.gl/wYuAXk'>axel.bodart</a>
                </div>
            </div>

            <div class="row-fluid">
                <div class="span1">
                    <i class="fa fa-github fa-fw"></i>
                </div>
                <div class="span11">
                    <a href='http://github.com/akeru'>akeru</a>
                </div>
            </div>

        </div>
    </div>
    <div class="span9">
        <div class="mainbar">
            <div class="section">
                <div class="section-job"><h3><i class="fa fa-briefcase hidden-print"></i>{{section.job}}</h3></div>
                {{#jobs}}
                    <div class="row">
                        <div class="span2">
                            <b>{{startdate.month}} {{startdate.year}} &mdash; {{enddate.month}} {{enddate.year}}</b>
                        </div>
                        <div class="span7">
                            <b>{{title}}</b> &mdash; <a href="{{company.url}}">{{company.name}}</a>
                        </div>
                    </div>
                    {{#projects}}
                        <div class="row">
                            <div class="span2">
                                <u>{{name}}</u>
                            </div>
                            <div class="span7">
                                {{description}}
                            </div>
                        </div>
                    {{/projects}}
                    {{#unless @last}}
                        <div class="row">
                            <div class="span9">
                                &nbsp;
                            </div>
                        </div>
                    {{/unless}}
                {{/jobs}}
            </div>
            <div class="section">
                <div class="section-skill"><h3><i class="fa fa-dashboard hidden-print"></i>{{section.skill}}</h3></div>
                <div class="row">
                    {{#skills}}
                        <div class="span3">
                            <b>{{name}}</b><br/>
                            {{#entries}}
                                {{name}}<br/>
                            {{/entries}}
                        </div>
                    {{/skills}}
                </div>
            </div>
            <div class="section">
                <div class="section-education"><h3><i class="fa fa-book hidden-print"></i>{{section.education}}</h3></div>
                {{#educations}}
                    <div class="row">
                        <div class="span3">{{name}}</div>
                        <div class="span3"><b>{{description}}</b></div>
                        <div class="span3">{{startdate.year}} &mdash; {{enddate.year}}</div>
                    </div>
                {{/educations}}
            </div>
            <div class="section">
                <div class="section-language"><h3><i class="fa fa-flag hidden-print"></i>{{section.language}}</h3></div>
                {{#languages}}
                    <div class="row">
                        <div class="span3">{{name}}</div>
                        <div class="span3">{{description}}</div>
                    </div>
                {{/languages}}
            </div>
            <div class="section">
                <div class="section-development"><h3><i class="fa fa-github-alt hidden-print"></i>{{section.development}}</h3></div>
                {{#development.entries}}
                    <div class="row">
                        <div class="span3"><a href="{{url}}">{{name}}</a></div>
                        <div class="span6">{{description}}</div>
                    </div>
                {{/development.entries}}
            </div>
            <div class="section">
                <div class="section-interest"><h3><i class="fa fa-beer hidden-print"></i>{{section.interest}}</h3></div>
                {{#interests}}
                    <div class="row">
                        <div class="span3">{{name}}</div>
                        <div class="span6">{{description}}</div>
                    </div>
                {{/interests}}
            </div>
        </div>
    </div>
</div>